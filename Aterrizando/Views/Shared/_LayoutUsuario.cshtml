<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Aterrizando</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/css/bootstrap.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">

                @Html.ActionLink("Aterrizando", "Index", "Usuario", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Historial", "Historial", "Usuario", new { area = "" }, new { @class = "navbar-brand" })




            </div>

            <div class="navbar-right">

                @Html.ActionLink("Logout", "Logout", "Usuario", new { area = "" }, new { @class = "navbar-brand" })

            </div>

            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav"></ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()

    </div>

    <footer class="navbar-fixed-bottom">
        <hr />
        <div class=" container body-content">
            <p>&copy; @DateTime.Now.Year - Aterrizando - PW3 Grupo 3</p>
        </div>
    </footer>
    <script src="~/js/jquery-1.10.2.min.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
</body>
</html>